{
  "name": "dumlj-build",
  "version": "0.0.1",
  "private": true,
  "description": "something build utilities",
  "license": "MIT",
  "workspaces": {
    "packages": [
      "@lib/*",
      "@feature/*",
      "@webpack-plugin/*",
      "@vite-plugin/*",
      "@cli/*"
    ]
  },
  "scripts": {
    "ci:pages": "shx rm -rf ./gh-pages && mkdir -p ./gh-pages && shx cp -rf ./coverage ./gh-pages/coverage && dumi build",
    "clear": "rimraf ./@*/*/{libs,build,tsconfig.tsbuildinfo}",
    "compile": "lerna exec --stream --scope @dumlj/* -- concurrently -P \"yarn:compile:* {1}\" --",
    "dev": "jest -c ./jest.config.ts --watch",
    "dev:docs": "dumi dev",
    "format": "prettier --config .prettierrc.js --write \"**/*.{js,jsx,ts,tsx,d.ts,vue,md,json,yml,yaml}\"",
    "preinstall": "./scripts/preinstall",
    "postinstall": "ts-patch install -s",
    "lint": "yarn lint:ts",
    "lint:ts": "eslint --config .eslintrc.js --ext .ts,.tsx --fix . --cache --cache-location \".eslintcache/\"",
    "test": "jest -c ./jest.config.ts --coverage --reporters=default --reporters=jest-junit"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.0",
    "@commitlint/config-conventional": "^17.4.0",
    "@jest/types": "^29.6.3",
    "@stackblitz/sdk": "^1.9.0",
    "@swc/helpers": "^0.5.2",
    "@tsd/typescript": "^5.2.2",
    "@types/jest": "^29.5.4",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "commitizen": "^4.2.4",
    "commitlint-plugin-i18n": "^0.0.1-alpha.3",
    "concurrently": "^8.2.1",
    "cz-customizable": "^7.0.0",
    "cz-customizable-i18n-config": "^0.0.1-alpha.2",
    "dumi": "^2.2.7",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react-hooks": "^4.4.0",
    "fs-extra": "^11.1.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-puppeteer": "^9.0.0",
    "jest-runner-tsd": "^6.0.0",
    "lerna": "^7.1.5",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "puppeteer": "^21.2.0",
    "rimraf": "^5.0.1",
    "shx": "^0.3.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-patch": "^3.0.2",
    "tsd-lite": "^0.8.0",
    "tslib": "^2.5.0",
    "typescript": "^5.2.2",
    "typescript-transform-paths": "^3.4.6",
    "utility-types": "^3.10.0"
  },
  "packageManager": "yarn@1.22.1"
}
